<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softark.eddie.gasexpress">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ge_laucher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GEMainActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".GESplashActivity"
            android:theme="@style/GESplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PriceActivity"
            android:theme="@style/PriceTheme"
            android:parentActivityName=".GEMainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GEMainActivity" />
        </activity>
    </application>

</manifest>