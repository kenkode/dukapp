<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.softark.eddie.gasexpress">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ge_laucher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:label="Gas Express"
            android:name=".GESplashActivity"
            android:theme="@style/GESplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PriceActivity"
            android:label="Gas Prices"
            android:parentActivityName=".GasExpress"
            android:theme="@style/PriceTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GasExpress" />
        </activity>
        <activity
            android:name=".PreviousPurchasesActivity"
            android:label="History"
            android:parentActivityName=".GasExpress"
            android:theme="@style/PriceTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GasExpress" />
        </activity>
        <activity
            android:name=".GEOrderActivity"
            android:label="Purchase"
            android:parentActivityName=".GasExpress"
            android:theme="@style/PriceTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GasExpress" />
        </activity>
        <activity
            android:name=".GERegisterActivity"
            android:label="Register"
            android:parentActivityName=".GasExpress"
            android:theme="@style/PriceTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GasExpress" />
        </activity>
        <activity
            android:windowSoftInputMode="adjustResize"
            android:name=".GELoginActivity"
            android:theme="@style/PriceTheme"
            android:label="Login" />
        <activity
            android:name=".GELocation"
            android:parentActivityName=".GEOrderActivity"
            android:theme="@style/PriceTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GEOrderActivity" />
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCQWEcHRAVHVNPQLfntrr7T3P6rmOfO4NI" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".GasExpress"
            android:label="@string/title_activity_gas_express"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".GEMyLocationActivity"
            android:label="My Locations"
            android:parentActivityName=".GEOrderActivity"
            android:theme="@style/PriceTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GasExpress" />
        </activity>
    </application>

</manifest>